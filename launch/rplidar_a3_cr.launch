<launch>
  <node name="rplidarNode"          pkg="rplidar_ros"  type="rplidarNode" output="screen">
    <param name="serial_port"         type="string" value="/dev/ttyUSB3"/>  
    <param name="serial_baudrate"     type="int"    value="256000"/><!--A3 -->
    <param name="frame_id"            type="string" value="laser_back"/>
    <param name="inverted"            type="bool"   value="false"/>
    <param name="angle_compensate"    type="bool"   value="true"/>
    <param name="scan_mode"           type="string" value="Sensitivity"/>
    <!--  Standard: max_distance: 16.0 m, Point number: 4.0K
          Express: max_distance: 16.0 m, Point number: 7.9K
          Boost: max_distance: 25.0 m, Point number: 15.9K
          Sensitivity: max_distance: 25.0 m, Point number: 15.9K
          Stability: max_distance: 25.0 m, Point number: 10.0K -->
    <remap from="/scan" to="/scan_back"/>

  </node>

    <node pkg="tf" type="static_transform_publisher" name="tf_base_rplidar"
    args="0 -0.152 0.2 0 0  0.0 /robot_base_footprint /laser_back 30" />

</launch>
